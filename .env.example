# 应用配置
APP_NAME=Nuwa
APP_VERSION=1.0.0
ENV=development
DEBUG=true
HOST=0.0.0.0
PORT=8000

# 数据库配置
DATABASE_URL=postgresql://nuwa:nuwa_password@localhost:5432/nuwa

# Redis配置
REDIS_URL=redis://localhost:6379/0

# 安全配置
SECRET_KEY=your-secret-key-here
ACCESS_TOKEN_EXPIRE_MINUTES=30

# 日志配置
LOG_LEVEL=INFO
LOG_FORMAT=json

# 插件配置
PLUGIN_DISCOVERY_INTERVAL=60
PLUGIN_TIMEOUT=30

# AI模型配置
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
